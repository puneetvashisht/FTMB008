<html>
<head>
    <title>Courses Page</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Title</th>
                <th>Summary</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i = 0; i < courses.length; i++) { %>
                <tr>
                    <td><%= courses[i].title %></td>
                    <td><%= courses[i].summary %></td>
                    </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>